public class Main
{
	public static void main(String[] args) {
	int n=5;
//Outer loop
	for(int i=1;i<=n;i++){
	    //Inner loop
	    for(int j=1;j<=i;j++){
	        //sum for finding even odd
	        int sum=i+j;
	       if(sum%2 ==0){//Even
	           System.out.print("1 ");
	       }    else{//Odd
	           System.out.print("0 ");
	       }
	    }
	    System.out.println();
	}
	}
}
