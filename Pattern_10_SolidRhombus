public class Main
{
	public static void main(String[] args) {
	    //printing a Solid Rhombus
	int n=5;
//Outer loop
	for(int i=1;i<=n;i++){
	    //Inner loop  --> for space
	    for(int j=1;j<=n-i;j++){
	        System.out.print(" ");
	    }
	    //Inner loop  --> for stars
	    for (int j=1;j<=n;j++){
	        System.out.print("*");
	    }
	    System.out.println();
	}
	}
}
