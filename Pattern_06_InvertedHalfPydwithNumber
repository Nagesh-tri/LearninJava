public class Main
{
	public static void main(String[] args) {
	int n=5;
	//for outer loop
  //just change in I
	for (int i=n;i>=1;i--){
	    //Inner loop 
	    for(int j=1;j<=i;j++){
	        //number should equal to j so we print J
	        System.out.print(j+" ");
	    }
	    //print a new line.
	    System.out.println();
	}
	}
}
